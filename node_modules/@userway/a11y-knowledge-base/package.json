{
  "name": "@userway/a11y-knowledge-base",
  "description": "A knowledge base for @userway/a11y-static-analysis package",
  "version": "0.0.7",
  "engines": {
    "node": ">=18.6.0"
  },
  "author": "UserWay Inc. <support@userway.org>",
  "homepage": "https://userway.org/",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.{js,mjs,d.ts}"
