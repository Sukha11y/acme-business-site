{
  "name": "@userway/cicd-cli",
  "version": "0.0.160",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "userway": "dist/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build:prod": "tsup",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix --cache",
    "lint:stats": "yarn lint --format ../../node_modules/eslint-stats/byErrorAndWarningStacked",
    "test": "jest --forceExit --silent --noStackTrace --ci --logHeapUsage --maxWorkers=10 --workerIdleMemoryLimit='300MB' --passWithNoTests",
    "test:ci": "jest --forceExit --silent --noStackTrace --ci --logHeapUsage --maxWorkers=10 --workerIdleMemoryLimit='300MB' --coverage --group=-skipOnCI --passWithNoTests",
    "valid": "yarn lint:fix && yarn build:prod"
  },
  "dependencies": {
    "@userway/cicd-api": "0.0.167",
    "@userway/cicd-core": "0.0.159",
    "commander": "^12.1.0",
    "jiti": "^1.21.6",
    "lockfile": "^1.0.4",
    "sucrase": "^3.35.0",
    "winston": "^3.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lockfile": "^1.0.4",
    "@types/node": "^20.14.10",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "gitHead": "26dbf441940c7de35e1ab9ffd66ee2b7e9c420c0"
}
